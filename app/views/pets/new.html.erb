<div class="container m-5">
  <h1>Add your pet 🐢</h1>
  <%= simple_form_for @pet do |f| %>
    <%= f.input :name %>
    <%= f.input :species, label: "widget" %>
    <%= f.input :address %>
    <%= f.input :found_on %>
    <%= f.button :submit %>
  <% end %>
</div>

